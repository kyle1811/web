    function ApplyDisplayColor() {
        $("#grid_order_result tr").each(function (e) {

            if (($(this).children().eq(4).text() == "토목 계") || ($(this).children().eq(4).text() == "건축 계") || ($(this).children().eq(4).text() == "해외 계")) {
