                $(this).children().each(function (e) {
                    $(this).css("font-weight", "bold");

                });
            }

            if ($(this).children().eq(4).text() == "총    합") {
                $(this).css("background-color", "#BDBDBD");

                $(this).children().each(function (e) {
                    $(this).css("font-weight", "bold");

                });

                $(this).children().each(function (e) {
                    $(this).css("color", "#191919");

                });
            }
        });
    }

