@{
    Layout = null;
    ViewBag.Title = "KSIS(KCC E&C SMART INFORMATION SYSTEM)";
}

<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <!-- 인코딩 -->
    <meta http-equiv="Contents-Type" content="text/html; charset=utf-8" />
    <!-- 파비콘 -->
    <link rel="shortcut icon" type="text/css" href="/Contents/images/icon/portal.ico" />
    <!-- css -->
    <link rel="stylesheet" type="text/css" href="/Contents/css/common.css" />
    <link rel="stylesheet" type="text/css" href="/Contents/css/portal.css" />
    <!-- script -->
    <script type="text/javascript" src="/Contents/js/common.js"></script>
    <script type="text/javascript" src="/Contents/js/jquery-1.9.1.min.js"></script>
</head>
<body>
    <!-- 전체 -->
    <div id="wrap">
        <!-- 본문 -->
        <div id="container">
            <!-- 콘텐츠 -->
            <div id="contents">

                <div id="area">

                </div>
               
            </div>
            <!-- //콘텐츠 -->
        </div>
        <!-- //본문 -->
    </div>
    <!-- //전체 -->
    <script type="text/javascript">

        

        // 페이지 로드 시
        $(document).ready(function (e) {
            AndroidLogin("kyle0829");

        });

        function AndroidLogin(pUserId) {

            $.ajax({
                type: "POST",
                url: "/Android/AndroidLogin",
                dataType: "json",
                data: { "pUserId": pUserId },
                async: false,   // 동기식 처리
                success: function (data) {
          
                        $("#area").empty();
                        $("#area").append("<p>" + data.UserId + "</p><p>" + data.UserNm + "</p>");
       
                },
                error: function (e) {
                    alert("에러 발생.");
                    return false;
                }
            });
        }
       

    </script>
</body>
</html>
